{
    "name": "organi/helpers",
    "description": "",
    "keywords": [
        "Organi",
        "laravel",
        "helpers"
    ],
    "homepage": "https://bitbucket.org/organi_team/helpers",
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Van Keilegom",
            "email": "rvk@organi.be",
            "homepage": "https://organi.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3",
        "nunomaduro/larastan": "^0.7.12",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/phpstan": "^0.12.99",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Organi\\Helpers\\": "src",
            "Organi\\Helpers\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Organi\\Helpers\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Organi\\Helpers\\HelpersServiceProvider"
            ],
            "aliases": {
                "Helpers": "Organi\\Helpers\\HelpersFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
